[alias]
  aa = add .
  ca = commit --amend
  cm = commit -m
  co = checkout
  cob = checkout -b
  com = !git checkout master || git checkout main
  ds = diff --staged
  pf = push --force
  plog = log --pretty=format:\"%C(auto)%h %C(cyan)%ad %C(green)%an%C(auto)%d %Creset%s\" --date=short
  puu = !git push -u origin \"$(git rev-parse --abbrev-ref HEAD)\" # https://superuser.com/a/1632310
  s = status
  ss = stash --staged
